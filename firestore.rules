rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /users/{uid} {
    	allow read, write: if request.auth != null && request.auth.uid == uid;
    }
    match /lobbies/{lobbyId} {
      allow read: if request.auth != null;
    }
  }
}
